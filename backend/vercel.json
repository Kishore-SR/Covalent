{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ],
  "env": {
    "JWT_SECRET_KEY": "@jwt_secret_key",
    "MONGODB_URI": "@mongodb_uri",
    "STREAM_API_KEY": "@stream_api_key",
    "STREAM_API_SECRET": "@stream_api_secret"
  }
}